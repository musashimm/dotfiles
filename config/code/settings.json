{
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.fontFamily": "'Consolas ligaturized v2','Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.fontLigatures": true,
  "editor.fontSize": 19,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": false,
  "editor.formatOnType": true,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "boundary",
  "editor.smoothScrolling": true,
  "editor.tabSize": 2,
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 120,
  "files.insertFinalNewline": true,
  "files.autoSave": "off",
  "files.exclude": {
    "/.git": true,
    "/.DS_Store": true,
    "/node_modules": true,
    "/node_modules/": true
  },
  "search.exclude": {
    "/node_modules": true
  },
  "files.watcherExclude": {
    "/node_modules/": true
  },
  "workbench.iconTheme": "vscode-icons",
  "workbench.colorTheme": "Gruvbox Dark Medium",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.colorCustomizations": {

    "statusBar.background": "#49467a",
    "statusBar.foreground": "#ffffff"
  },
  "window.titleBarStyle": "custom",
  "search.showLineNumbers": true,
  "breadcrumbs.enabled": true,
  "ruby.lint": {
    "rubocop": true
  },
  "ruby.intellisense": "rubyLocate",
  "ruby.locate": {
  "include": "**/*.rb",
  "exclude": "{**/@(test|spec|tmp|.*),**/@(test|spec|tmp|.*)/**,**/*_spec.rb}"
  },
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "eslint.packageManager": "yarn",
  "npm.packageManager": "yarn",
  "telemetry.telemetryLevel":"off",
  "solargraph.logLevel": "debug",
  "solargraph.diagnostics": true,
  "rubyTestExplorer.testFramework": "minitest",
  "files.trimTrailingWhitespace": true,
  "files.trimFinalNewlines": true,
  "update.mode": "none",
  "hediet.vscode-drawio.local-storage": "",
  "window.zoomLevel": 0,
  "redhat.telemetry.enabled": false,
  "files.associations": {
    "*.jb": "ruby"
  }
}
